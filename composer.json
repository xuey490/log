    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "log",
        "logger",
        "monolog",
        "fssphp"
    ],
    "authors": [
        {
            "name": "xuey863toy",
            "email": "xuey863toy@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "monolog/monolog": "^3.9",
        "psr/log": "^3.0",
        "symfony/http-foundation": "^7.3"
    },
    "autoload": {
        "psr-4": {
            "Framework\\Log\\": "src/"
        }
